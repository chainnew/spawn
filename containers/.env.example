# Spawn Agent Container Environment Variables
# Copy to .env and fill in your values

# ==============================================
# API Keys (at least one required)
# ==============================================

# xAI API Key (recommended for Grok 4.1)
XAI_API_KEY=xai-xxxxxxxxxxxxxxxxxxxxx

# OpenRouter API Key (alternative)
# OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxxxxxxxxxx

# ==============================================
# Database
# ==============================================

# PostgreSQL password (change in production!)
POSTGRES_PASSWORD=spawn_dev_password

# ==============================================
# Ports (optional, defaults shown)
# ==============================================

# AGENT_PORT=3080
# REDIS_PORT=6379
# POSTGRES_PORT=5432

# ==============================================
# Production Settings
# ==============================================

# NODE_ENV=production

# Domain for HTTPS (uncomment for production)
# DOMAIN=spawn.example.com
